    "next-auth": "^4.10.2",
    "next-themes": "^0.0.14",
    "nodemailer": "^6.7.7",
    "nuka-carousel": "^5.1.3",
    "postcss": "^8.4.5",
    "preact": "^10.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-iframe": "^1.8.0",
    "reading-time": "1.3.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^0.4.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.1.3",
