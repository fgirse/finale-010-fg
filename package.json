    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "@next-auth/mongodb-adapter": "^1.0.4",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "10.4.5",
    "esbuild": "^0.13.13",
    "github-slugger": "^1.3.0",
    "gray-matter": "^4.0.2",
    "gsap": "^3.10.4",
    "image-size": "1.0.0",
